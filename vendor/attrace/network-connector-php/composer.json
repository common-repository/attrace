{
  "name": "attrace/network-connector-php",
  "description": "Attrace PHP module",
  "keywords": [
    "Attrace",
    "php"
  ],
  "homepage": "https://gitlab.com/attrace/network-connector-php",
  "license": "MIT",
  "authors": [
    {
      "name": "Attrace Team",
      "homepage": "http://www.attrace.network",
      "role": "Developer"
    }
  ],
  "require": {
	"php": ">=5.6",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "google/protobuf": "^3.12",
    "paragonie/sodium_compat": "^1.13",
    "phpseclib/phpseclib": "^2.0",
    "phpseclib/bcmath_compat": "^1.0"
  },
  "config": {
    "sort-packages": true
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Attrace\\Connector\\": "src",
      "Google\\Protobuf\\": "src/Google/Protobuf",
      "GPBMetadata\\Google\\Protobuf\\": "src/GPBMetadata/Google/Protobuf"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Attrace\\Connector\\Test\\": "tests"
    }
  }
}
